@inherits Blazorise.Bar
<CascadingValue Value="@this" IsFixed>
    <CascadingValue Value="@State">
        <ul @ref="@ElementRef" id="@ElementId" class="@ClassNames" style="@StyleNames" direction="ltr" role="menu" @attributes="@Attributes">
            @ChildContent
        </ul>
    </CascadingValue>
</CascadingValue>
