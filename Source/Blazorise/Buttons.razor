@inherits BaseComponent
@if ( !HasCustomRegistration )
{
    <CascadingValue Value="this">
        <div class="@ClassNames" role="@(Role== ButtonsRole.Toolbar? "toolbar" : "group")" @attributes="@Attributes">
            @ChildContent
        </div>
    </CascadingValue>
}
else
{
    @RenderCustomComponent()
}