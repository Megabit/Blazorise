@typeparam TValue
@inherits BaseInputComponent<IReadOnlyList<TValue>>
@if ( !HasCustomRegistration )
{
    <select @ref="@ElementRef" id="@ElementId" class="@ClassNames" style="@StyleNames" disabled="@Disabled" readonly="@ReadOnly" multiple="@Multiple" size="@MaxVisibleItems" value="@CurrentValueAsString" @onchange="@OnChangeHandler" @onfocus="@OnFocus" @onfocusin="@FocusIn" @onfocusout="@FocusOut" @attributes="@Attributes">
        <CascadingValue Value=this>
            @ChildContent
        </CascadingValue>
    </select>
    @Feedback
}
else
{
    @RenderCustomComponent()
}