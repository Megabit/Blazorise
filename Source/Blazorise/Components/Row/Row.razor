@namespace Blazorise
@inherits BaseRowComponent
<div id="@ElementId" class="@ClassNames" style="@StyleNames" @attributes="@Attributes">
    <CascadingValue Value="@this" IsFixed>
        <CascadingValue Value="@RowContext" IsFixed>
            <CascadingValue Value="@GutterState">
                @ChildContent
            </CascadingValue>
        </CascadingValue>
    </CascadingValue>
</div>