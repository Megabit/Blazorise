@namespace Blazorise
@inherits BaseComponent
@if ( ReadOnly )
{
    <Icon Class="@ClassNames" Style="@StyleNames" Name="@IconName" IconStyle="@IconStyle"></Icon>
}
else
{
    <Icon Class="@ClassNames" Style="@StyleNames" Name="@IconName" IconStyle="@IconStyle" Clicked="@HandleClick" MouseOver="@HandleMouseOver" MouseOut="@HandleMouseOut" />
}