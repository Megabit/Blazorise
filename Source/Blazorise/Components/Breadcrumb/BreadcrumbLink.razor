@namespace Blazorise
@inherits BaseComponent
@if ( IsActive )
{
    @ChildContent
}
else
{
    <Blazorise.Link ElementId="@ElementId" Class="@ClassNames" Style="@StyleNames" To="@To" Target="@Target" Title="@Title" Match="@Match" Clicked="@ClickHandler" Attributes="@Attributes">
        @ChildContent
    </Blazorise.Link>
}