@namespace Blazorise
@inherits BaseComponent
<CascadingValue Value="@this" IsFixed="true">
    <CascadingValue Value="@State">
        <div @ref="@ElementRef" id="@ElementId" class="@ClassNames" style="@StyleNames" @attributes="@Attributes" @onmouseleave="MouseLeave" @onmouseenter="MouseEnter">
            @ChildContent
        </div>
    </CascadingValue>
</CascadingValue>