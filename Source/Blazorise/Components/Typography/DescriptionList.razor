@namespace Blazorise
@inherits BaseTypographyComponent
<CascadingValue Value="@this" IsFixed>
    <CascadingValue Value="@RowContext" IsFixed>
        <dl @ref="@ElementRef" id="@ElementId" class="@ClassNames" style="@StyleNames" @onclick="@OnClickHandler" @attributes="@Attributes">
            @ChildContent
        </dl>
    </CascadingValue>
</CascadingValue>