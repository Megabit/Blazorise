@namespace Blazorise
@typeparam TValue
@inherits BaseInputComponent<TValue>
@attribute [CascadingTypeParameter(nameof(TValue))]
<CascadingValue Value="@this" IsFixed>
    <div id="@ElementId" class="@ClassNames" style="@StyleNames" tabindex="@TabIndex" @onkeydown="@OnKeyDownHandler" @onkeypress="@OnKeyPressHandler" @onkeyup="@OnKeyUpHandler" @onblur="@OnBlurHandler" @onfocus="@OnFocusHandler" @onfocusin="@OnFocusInHandler" @onfocusout="@OnFocusOutHandler" @attributes="@Attributes">
        @ChildContent
    </div>
    @Feedback
</CascadingValue>