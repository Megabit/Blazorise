@inherits Base.BaseCheckEdit
@if ( !ComponentMapper.HasRegistration( this ) )
{
    <input type="@Type" class="@ClassMapper.Class" style="@StyleMapper.Style" disabled="@IsDisabled" readonly="@IsReadonly" checked="@Checked" onchange="@HandleCheckedChanged" name="@RadioGroup" id="@ElementId" />
    @ChildContent
    @Feedback
}
else
{
    @RenderCustomComponent()
}