@inherits BaseComponent
@if ( !HasCustomRegistration )
{
    @if ( IconName is RenderFragment )
    {
        <div class="@ClassNames" style="@StyleNames">
            @( IconName as RenderFragment )
        </div>
    }
    else
    {
        <Icon Name="@IconName" IconStyle="@IconStyle" Class="@ClassNames" Style="@StyleNames" />
    }
}
else
{
    @RenderCustomComponent()
}