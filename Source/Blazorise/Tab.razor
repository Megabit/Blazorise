@inherits BaseComponent
@if ( !HasCustomRegistration )
{
    <li class="@ClassNames" style="@StyleNames" @attributes="@Attributes">
        <a class="@LinkClassNames" @onclick="@ClickHandler" tabindex="@(Disabled ? "-1" : "0")" aria-disabled="@(Disabled.ToString().ToLowerInvariant())">@ChildContent</a>
    </li>
}
else
{
    @RenderCustomComponent()
}