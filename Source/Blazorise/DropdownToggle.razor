@inherits Blazorise.BaseDropdownToggle
@if ( !HasCustomRegistration )
{
    <button id="@ElementId" type="button" class="@ClassNames" style="@StyleNames" data-toggle="dropdown" @onclick="@ClickHandler" @attributes="@Attributes">
        @ChildContent
    </button>
}
else
{
    @RenderCustomComponent()
}