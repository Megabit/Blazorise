@inherits Blazorise.BaseFileEdit
@if ( !HasCustomRegistration )
{
    <input id="@ElementId" type="file" class="@ClassNames" style="@StyleNames" disabled="@IsDisabled" readonly="@IsReadonly" multiple="@IsMultiple" accept="@Filter" @onchange="@OnChangeHandler" @attributes="@Attributes" />
    @ChildContent
}
else
{
    @RenderCustomComponent()
}