<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:Blazorise.Demo.MAUI"
             xmlns:demo="clr-namespace:Blazorise.Demo;assembly=Blazorise.Demo"
             x:Class="Blazorise.Demo.MAUI.MainPage"
             BackgroundColor="{DynamicResource PageBackgroundColor}">

    <BlazorWebView HostPage="wwwroot/index.html">
        <BlazorWebView.RootComponents>
            <RootComponent Selector="#app" ComponentType="{x:Type demo:App}" />
        </BlazorWebView.RootComponents>
    </BlazorWebView>

</ContentPage>